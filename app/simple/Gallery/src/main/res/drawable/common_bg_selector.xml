<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_activated="true" android:drawable="@drawable/common_bg_stroke"/>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/divider_color" />
            <stroke
                android:width="1dp"
                android:color="@color/normal_black"/>
        </shape>
    </item>
    <item android:state_pressed="true" android:drawable="@drawable/common_bg_stroke"/>
    <item>
        <shape>
            <solid android:color="@android:color/white"/>
            <stroke
                android:width="1dp"
                android:color="@color/normal_black"/>
        </shape>
    </item>

</selector>
